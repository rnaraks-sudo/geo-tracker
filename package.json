{
  "name": "tracker-co2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx http-server ./www -c-1 --port 8080",
    "build": "echo 'PWA already built in /www';",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:copy": "npx cap copy",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "android:build": "npm run cap:copy && npx cap open android",
    "ios:build": "npm run cap:copy && npx cap open ios"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/geolocation": "^6.0.1"
  }
}